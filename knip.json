{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/cli/index.ts"],
  "project": ["src/**/*.ts"],
  "ignore": ["tests/e2e/projects/**/*", ".github/scripts/**/*", "scripts/**/*"],
  "ignoreBinaries": ["ts-node"],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "unlisted": "warn",
    "unresolved": "error",
    "exports": "warn",
    "types": "warn"
  }
}
